<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	version="2.5"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>DoAjaxServlet</servlet-name>
		<servlet-class>com.xgbxm.model.DoAjaxServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DoAjax</servlet-name>
		<servlet-class>com.xgbxm.model.DoAjax</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DoAjax</servlet-name>
		<url-pattern>/ajax.do</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>DoAjaxServlet</servlet-name>
		<url-pattern>/validate.do</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>